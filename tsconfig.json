{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "types": [
      "nuxt"
    ]
  },
  "include": [
    ".nuxt/nuxt.d.ts",
    "**/*"
  ]
}
